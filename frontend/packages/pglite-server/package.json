{
  "name": "@liam-hq/pglite-server",
  "private": true,
  "version": "0.0.0",
  "publishConfig": {
    "access": "restricted"
  },
  "main": "src/index.ts",
  "dependencies": {
    "@electric-sql/pglite": "0.3.6",
    "@pgsql/types": "15.1.1",
    "neverthrow": "8.2.0",
    "pg-query-emscripten": "5.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@liam-hq/configs": "workspace:*",
    "eslint": "9.32.0",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  },
  "scripts": {
    "build": "tsc",
    "fmt": "concurrently \"pnpm:fmt:*\"",
    "fmt:biome": "biome check --write --unsafe .",
    "fmt:eslint": "eslint --fix .",
    "lint": "concurrently \"pnpm:lint:*\"",
    "lint:biome": "biome check .",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "test": "vitest"
  }
}
