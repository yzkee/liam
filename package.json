{
  "name": "liam-frontend",
  "private": true,
  "version": "0.0.1",
  "devDependencies": {
    "@changesets/cli": "2.29.7",
    "@changesets/get-github-info": "0.6.0",
    "@changesets/types": "6.1.0",
    "@turbo/gen": "2.5.8",
    "@types/node": "22.18.11",
    "@vitest/coverage-v8": "3.2.4",
    "concurrently": "9.2.1",
    "knip": "5.66.0",
    "lefthook": "1.13.6",
    "stylelint": "16.25.0",
    "stylelint-config-recess-order": "7.4.0",
    "stylelint-value-no-unknown-custom-properties": "6.0.1",
    "syncpack": "13.0.4",
    "turbo": "2.5.8",
    "typescript": "5.9.3",
    "vercel": "41.7.8",
    "vitest": "3.2.4"
  },
  "scripts": {
    "build": "turbo build",
    "create-env-files": "([ -f .env ] || touch .env) && ([ -f .env.local ] || touch .env.local)",
    "dev": "turbo dev",
    "fmt": "concurrently \"pnpm:fmt:*\"",
    "fmt:knip": "knip --fix",
    "fmt:syncpack-fix": "syncpack fix-mismatches",
    "fmt:syncpack-format": "syncpack format",
    "fmt:turbo": "turbo fmt",
    "gen:turbo": "turbo gen",
    "lint": "pnpm lint:turbo && pnpm lint:syncpack && pnpm lint:knip",
    "lint:knip": "knip --treat-config-hints-as-errors",
    "lint:stylelint": "stylelint \"frontend/**/*.css\" --ignore-path .stylelintignore --max-warnings 0",
    "lint:syncpack": "syncpack lint",
    "lint:turbo": "turbo lint",
    "prebuild": "pnpm create-env-files",
    "prelint": "pnpm create-env-files",
    "prepare": "lefthook install",
    "release": "pnpm build --filter @liam-hq/cli && pnpm changeset publish",
    "test": "turbo test",
    "test:coverage": "vitest --coverage",
    "test:e2e": "turbo test:e2e",
    "vercel:env-pull": "vercel env pull .env",
    "vercel:link": "vercel link -p liam-app -S \"liambx\" -y"
  },
  "packageManager": "pnpm@10.18.3",
  "pnpm": {
    "onlyBuiltDependencies": [
      "supabase"
    ],
    "overrides": {
      "@radix-ui/react-dialog": "1.1.15",
      "cookie": "^0.7.0",
      "esbuild": "0.25.11",
      "path-to-regexp": "8.3.0",
      "prismjs": "1.30.0",
      "undici": "7.16.0"
    }
  }
}
